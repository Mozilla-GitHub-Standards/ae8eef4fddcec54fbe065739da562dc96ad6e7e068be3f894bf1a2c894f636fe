{
  "name": "electron-genesis",
  "version": "1.0.0",
  "description": "Electron and essentials to get you started writing desktop apps.",
  "keywords": [
    "electron",
    "react",
    "parcel",
    "jest",
    "eslint",
    "prettier",
    "flow",
    "styled-components",
    "material"
  ],
  "homepage": "https://github.com/mozillasecurity/electron-genesis",
  "bugs": {
    "url": "https://github.com/mozillasecurity/electron-genesis/issues"
  },
  "license": "MPL-2.0",
  "author": "Christoph Diehl <cdiehl@mozilla.com>",
  "repository": "github:mozillasecurity/electron-genesis",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "main": "dist/main/main.js",
  "scripts": {
    "start": "run-p -r parcel:launch electron:launch",
    "dist": "run-s parcel:build electron:build",
    "dist-universal": "run-s parcel:build electron:build:universal",
    "electron:launch": "run-s parcel:main electron:start",
    "electron:start": "cross-env ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron:build": "build --dir --x64 --publish=never",
    "electron:build:universal": "build --dir --x64 --publish=never -mwl",
    "parcel:launch": "node parcel-bundler.js",
    "parcel:build": "run-s parcel:main parcel:renderer",
    "parcel:main": "parcel build src/main/main.js -d dist/main --out-file=main --target=electron",
    "parcel:renderer": "parcel build src/renderer/index.html --public-url ./ -d dist/renderer/production --target=electron",
    "test": "run-s parcel:build test:jest",
    "test:jest": "cross-env CI=true jest",
    "test:update": "yarn test:jest -u",
    "test:coverage": "yarn test:jest --silent --coverage",
    "typecheck": "flow check --quiet --color=always",
    "typecheck:init": "flow-typed install --overwrite",
    "typecheck:update": "flow-typed update",
    "typecheck:upgrade": "flow-upgrade",
    "typecheck:coverage": "flow-coverage-report --config=./.flowcoverage.json",
    "lint": "run-s -c typecheck lint:js lint:css",
    "lint:js": "eslint . --ext .js,.jsx --fix",
    "lint:css": "stylelint --formatter=verbose './src/**/*.jsx' --fix",
    "format": "run-s -c format:js format:css format:other",
    "format:js": "prettier-eslint --write './src/**/*.{js,jsx}'",
    "format:css": "prettier --write './src/**/*.{js,jsx}'",
    "docs": "esdoc",
    "updates": "ncu -t"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ],
      "*.{json,md}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "devDependencies": {
    "@jest-runner/electron": "0.0.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-styled-components": "^1.7.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.0",
    "electron": "^2.0.9",
    "electron-builder": "^20.28.4",
    "esdoc": "^1.1.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-node": "^1.0.3",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.81.0",
    "flow-coverage-report": "^0.6.0",
    "flow-typed": "^2.5.1",
    "flow-upgrade": "^1.0.4",
    "husky": "^1.0.0-rc.14",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "lint-staged": "^7.2.2",
    "npm-check-updates": "^2.14.2",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.2",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.5.1",
    "regenerator-runtime": "^0.12.1",
    "spectron": "^4.0.0",
    "stylelint": "^9.5.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.3",
    "@material-ui/icons": "^3.0.1",
    "app-root-path": "^2.1.0",
    "dotenv": "^6.0.0",
    "dotenv-expand": "^4.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^2.2.17",
    "electron-updater": "^3.1.2",
    "express": "^4.16.3",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^3.4.6",
    "styled-theming": "^2.2.0",
    "typeface-roboto": "^0.0.54"
  }
}
